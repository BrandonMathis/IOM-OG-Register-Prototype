<div class = "ccom_entity">
<%form_for(@asset) do |f| -%>
	<%= f.error_messages %>
	<table id="ccom-entity">
  	<% edit_ccom_entity.attribute_names.each do |field| -%>
      	<tr><td><%= field.to_s.camelize %></td>
			<td> 
				<% if edit_ccom_entity.editable_attribute_names.include?(field) -%>
					<%= f.text_field field.to_s.parameterize.underscore.to_sym, :size => '62' %>
				<% else -%>
					<%= edit_ccom_entity.send(field) %>
				<%end-%>
			</td>
		</tr>
  	<% end -%>
	</table>
	<%= f.submit "Update" -%>
<% end -%>
</div>