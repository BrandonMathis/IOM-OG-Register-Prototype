<ul>
	<% @asset.each do |asset| -%>
		<table>
			<h4><%=asset.tag%></h4>
	  		<% asset.attribute_names.each do |field| -%>
	    		<% unless (value = asset.send(field)).blank? %>
	      			<tr><td><%= field.to_s.titleize %></td><td> <%= value %></td></tr>
	    		<% end -%>
	  		<% end -%>
		</table>
	<% end -%>
</ul>
